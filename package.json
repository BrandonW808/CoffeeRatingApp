    "react-router-dom": "^6.17.0",
    "axios": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.1",
    "@babel/preset-react": "^7.24.1",
    "babel-loader": "^9.1.3",
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^5.91.0",
